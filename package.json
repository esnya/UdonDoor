{
  "name": "com.nekometer.udon-door",
  "version": "1.0.0-alpha.1",
  "private": true,
  "description": "UdonDoor",
  "keywords": [
    "VRChat",
    "Udon",
    "Unity"
  ],
  "homepage": "https://github.com/esnya/UdonDoor#readme",
  "bugs": {
    "url": "https://github.com/esnya/UdonDoor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esnya/UdonSunController.git"
  },
  "license": "MIT",
  "author": "esnya",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/core": "^11.0.0",
    "@semantic-release/github": "^7.2.0",
    "commitlint-config-gitmoji": "^2.1.10",
    "husky": "^4.3.8",
    "semantic-release": "^17.3.7",
    "semantic-release-gitmoji": "^1.3.4"
  },
  "commitlint": {
    "extends": [
      "gitmoji"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "semantic-release-gitmoji",
      [
        "@semantic-release/github",
        {
          "assets": {
            "path": "dist/"
          }
        }
      ]
    ]
  }
}
